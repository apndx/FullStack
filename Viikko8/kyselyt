query countBooksAndAuthors{
  bookCount
  authorCount
}

query listAllBooksWithDetails{
  allBooks { 
    title 
    author
    published 
    genres
  }
}

query listAllAuthorsWithNameAndBookCount{
  allAuthors {
    name
    bookCount
  }
}

query listAllBooksByAuthor {
  allBooks(author: "Robert Martin") {
    title
  }
}

query listAllBooks {
  allBooks {
    title
  }
}